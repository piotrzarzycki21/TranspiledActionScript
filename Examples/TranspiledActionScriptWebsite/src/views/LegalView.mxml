<mdl:Grid xmlns:fx="http://ns.adobe.com/mxml/2009"
		  xmlns:js="library://ns.apache.org/royale/basic"
		  xmlns:mdl="library://ns.apache.org/royale/mdl"
		  xmlns:html="library://ns.apache.org/royale/html">
	<mdl:style>
		<js:AllCSSStyles paddingTop="50" paddingRight="40"/>
	</mdl:style>
	<mdl:GridCell column="12" className="verticalFlexGroup">
		<mdl:style>
			<js:AllCSSStyles flexDirection="column" alignItems="flex-end" />
		</mdl:style>
		<mdl:Card className="demo-card-wide">
		    <mdl:style>
				<js:AllCSSStyles background="transparent" minHeight="auto"/>
			</mdl:style>	
			<mdl:CardTitle>
				<mdl:style>
					<js:AllCSSStyles flexDirection="column"/>
				</mdl:style>	
				<mdl:CardTitleText text="LEGAL">
					<mdl:style>
						<js:SimpleCSSStyles fontFamily="Poppins" color="#444444" 
											fontSize="36pt"/>
					</mdl:style>
				</mdl:CardTitleText>
			</mdl:CardTitle>		
		</mdl:Card>	
		<html:Hr percentWidth="100">
			<html:style>
				<js:AllCSSStyles marginRight="-31%" backgroundColor="#CFCFCF"/>
			</html:style>	
		</html:Hr>
		<mdl:Card className="demo-card-wide" percentWidth="70">
			<mdl:style>
				<js:AllCSSStyles background="transparent" minHeight="auto"/>
			</mdl:style>	
			<mdl:CardTitle>
				<mdl:style>
						<js:AllCSSStyles />
					</mdl:style>	
				<mdl:CardTitleText text="Source Code License">
