					<mdl:style>
						<js:SimpleCSSStyles fontFamily="Poppins" color="#444444" 
											fontSize="14pt" fontWeight="bold"/>
					</mdl:style>
				</mdl:CardTitleText>
			</mdl:CardTitle>
			<mdl:CardSupportingText percentWidth="100">
				<mdl:style>
					<js:AllCSSStyles fontFamily="Sintony" fontSize="14pt"
