<?xml version="1.0" encoding="utf-8"?>
<mdl:FooterLinkItemMouseRenderer xmlns:fx="http://ns.adobe.com/mxml/2009"
			  				xmlns:js="library://ns.apache.org/flexjs/basic"
			  				xmlns:mdl="library://ns.apache.org/flexjs/mdl"
						    href="{navigationVO.href}">
			
	<mdl:beads>
        <js:ItemRendererDataBinding />
    </mdl:beads>
	
	<fx:Script>
	<![CDATA[
		import vos.NavigationVO;
		
		[Bindable("dataChange")]
		public function get navigationVO():NavigationVO
		{
			return data as NavigationVO;
		}	
	]]>
	</fx:Script>
</mdl:FooterLinkItemMouseRenderer>