<mdl:TabBarPanelItemRenderer 
	xmlns:fx="http://ns.adobe.com/mxml/2009"
    xmlns:mdl="library://ns.apache.org/royale/mdl"
	xmlns:s="library://ns.apache.org/royale/basic">
    <mdl:beads>
        <s:ItemRendererDataBinding />
    </mdl:beads>
    <fx:Script>
        <![CDATA[
            import vos.TabVO;

            [Bindable("dataChange")]
            public function get tab():TabVO
            {
                return data as TabVO;
            }
        ]]>
    </fx:Script>

    <s:Label text="{tab.label}"/>
</mdl:TabBarPanelItemRenderer>