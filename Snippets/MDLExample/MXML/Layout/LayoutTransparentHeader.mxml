<mdl:NavigationLayout className="layoutTransparent">
	<mdl:Header>
		<mdl:HeaderRow>
			<mdl:LayoutTitle text="Title"/>
			<mdl:Spacer />
		</mdl:HeaderRow>
		<mdl:Navigation labelField="link">
			<mdl:beads>
				<js:ConstantBinding
					sourceID="model"
					sourcePropertyName="layoutData"
					destinationPropertyName="dataProvider"/>
			</mdl:beads>
		</mdl:Navigation>
	</mdl:Header>
	<mdl:Drawer>
		<mdl:LayoutTitle text="Title"/>
		<mdl:Navigation labelField="link">
			<mdl:beads>
				<js:ConstantBinding
					sourceID="model"
					sourcePropertyName="layoutData"
					destinationPropertyName="dataProvider"/>
			</mdl:beads>
		</mdl:Navigation>
	</mdl:Drawer>
	<mdl:NavigationLayoutContent>
	</mdl:NavigationLayoutContent>
</mdl:NavigationLayout>